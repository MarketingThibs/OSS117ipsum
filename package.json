{
	"name": "oss117psum",
	"displayName": "OSS117psum",
	"description": "Copiez dans le presse papier le meilleur des citations d'OSS 117 pour en faire du OSS117psum",
	"publisher": "marketingthibs",
	"icon": "oss117.png",
	"galleryBanner": {
		"color": "#333",
		"theme": "dark"
	},
	"author": {
		"name": "MarketingThibs"
	},
	"repository": {
        "type": "git",
        "url": "https://github.com/MarketingThibs/OSS117ipsum"
    },
	"license": "SEE LICENSE IN LICENSE.txt",
	"keywords":["OSS","117","OSS117","ipsum","lorem"],
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:oss117psum.titre",
		"onCommand:oss117psum.oneParagraph",
		"onCommand:oss117psum.twoParagraphs",
		"onCommand:oss117psum.twoParagraphsDialog",
		"onCommand:oss117psum.threeParagraphs",
		"onCommand:oss117psum.oneSentence"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "oss117psum.titre",
				"title": "OSS Titre"
			},
			{
				"command": "oss117psum.oneParagraph",
				"title": "OSS 1 Paragraphe"
			},
			{
				"command": "oss117psum.twoParagraphs",
				"title": "OSS 2 Paragraphes"
			},
			{
				"command": "oss117psum.twoParagraphsDialog",
				"title": "OSS 2 Paragraphes avec dialogue"
			},
			{
				"command": "oss117psum.threeParagraphs",
				"title": "OSS 3 Paragraphes"
			},
			{
				"command": "oss117psum.oneSentence",
				"title": "OSS 1 phrase"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
